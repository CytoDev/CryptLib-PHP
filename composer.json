{
    "name": "cytodev/lib-cryptography",
    "description": "library that implements anything regarding cryptography.",
    "keywords": [
        "library",
        "cryptography"
    ],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Roel Walraven",
            "email": "mail@cytodev.io"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": "^7.2",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "cytodev\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpunit/php-invoker": "^2.0"
    }
}
